{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/Product.jsx", "components/Navbar.jsx", "components/Layout.jsx", "components/FooterBanner.jsx", "components/Footer.jsx", "components/Cart.jsx", "components/HeroBanner.jsx", "pages/index.js", "pages/_app.jsx"],
  "exclude": ["node_modules"]
}
